  - ../../base

patches:
# Change the Subscription details
- target:
    kind: Subscription
    name: .*
  patch: |-
    - op: replace
      path: /spec/channel
      value: ebdn-latest
    - op: replace
      path: /spec/source
      value: ebdn-cs-my-redhat-catalog
